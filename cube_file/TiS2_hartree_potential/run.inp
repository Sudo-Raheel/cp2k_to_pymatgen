&GLOBAL
  RUN_TYPE  ENERGY
  PROJECT_NAME  run
  PRINT_LEVEL  MEDIUM
&END GLOBAL
&FORCE_EVAL
  METHOD  QUICKSTEP
  &DFT
    BASIS_SET_FILE_NAME  GTH_BASIS_SETS
    BASIS_SET_FILE_NAME  BASIS_MOLOPT_UCL
    POTENTIAL_FILE_NAME  GTH_POTENTIALS

    &POISSON
         PERIODIC XYZ
    &END POISSON
    &XC
      &XC_FUNCTIONAL  PBE
      &END XC_FUNCTIONAL
    &END XC
    CHARGE  0
    MULTIPLICITY  0
    &QS
      METHOD  GPW
    &END QS
    SURFACE_DIPOLE_CORRECTION .TRUE.
    SURF_DIP_DIR  Z
    SURF_DIP_POS  20
    &SCF
      SCF_GUESS  ATOMIC
      MAX_SCF  100
      EPS_SCF  1.0E-6
      ADDED_MOS 50
      &DIAGONALIZATION
        ALGORITHM  STANDARD
      &END DIAGONALIZATION
      &MIXING
        METHOD BROYDEN_MIXING
        ALPHA 0.1
        BETA 1.5
      &END MIXING
      &SMEAR
        METHOD FERMI_DIRAC
        FIXED_MAGNETIC_MOMENT 0.0
      &END SMEAR
    &END SCF
    &MGRID
      NGRIDS  4
      CUTOFF  550
      REL_CUTOFF  70
    &END MGRID
    &PRINT
      &MO
        OCCUPATION_NUMBERS .TRUE.
	MO_INDEX_RANGE 0 200
	EIGENVALUES .TRUE.
	&EACH
          QS_SCF 50
        &END EACH
        ADD_LAST NUMERIC
      &END MO
     &V_HARTREE_CUBE
	&EACH
  	 QS_SCF 100
	&END EACH
	FILENAME Hartree
     &END V_HARTREE_CUBE
    &END PRINT
  &END DFT
  &SUBSYS
    &TOPOLOGY
     COORD_FILE_FORMAT  XYZ
     COORD_FILE_NAME  ./config.xyz
     #&CENTER_COORDINATES
     #&END CENTER_COORDINATES
    &END TOPOLOGY
    &CELL
      ABC  13.740 13.740 30.00
      ALPHA_BETA_GAMMA 90.00 90.00 60.00
      PERIODIC XYZ
    &END CELL
    &KIND Ti
      ELEMENT Ti	
      BASIS_SET  TZV2P-MOLOPT-SR-GTH-q12
      POTENTIAL  GTH-PBE-q12
    &END KIND
    &KIND S
      ELEMENT S
      BASIS_SET  TZV2P-GTH-q6
      POTENTIAL  GTH-PBE-q6
    &END KIND
  &END SUBSYS
  &PRINT
    &TOTAL_NUMBERS  ON
    &END TOTAL_NUMBERS
  &END PRINT
&END FORCE_EVAL
